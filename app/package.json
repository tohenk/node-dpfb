{
  "name": "@ntlab/dpfb",
  "version": "3.1.3",
  "description": "A fingerprint acquisition and identification in the browser environment",
  "productName": "Digital Persona Fingerprint Bridge",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "build": "cd node_modules/@ntlab/dplib && npm run install",
    "build:32": "cd node_modules/@ntlab/dplib && npm run install:32",
    "build:64": "cd node_modules/@ntlab/dplib && npm run install:64",
    "package": "electron-forge package",
    "package:32": "electron-forge package --arch=ia32",
    "package:64": "electron-forge package --arch=x64",
    "make": "electron-forge make",
    "make:32": "electron-forge make --arch=ia32",
    "make:64": "electron-forge make --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tohenk/node-dpfb.git"
  },
  "keywords": [
    "digital persona",
    "fingerprint",
    "match",
    "verify",
    "identify",
    "enroll"
  ],
  "author": "Toha <tohenk@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tohenk/node-dpfb/issues"
  },
  "homepage": "https://github.com/tohenk/node-dpfb#readme",
  "dependencies": {
    "@ntlab/identity-fingerprint": "^1.1.1",
    "@ntlab/ntlib": "^2.6.2",
    "@ntlab/work": "^2.3.0",
    "jszip": "^3.10.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron/rebuild": "^3.7.2",
    "electron": "^22.3.27"
  }
}
